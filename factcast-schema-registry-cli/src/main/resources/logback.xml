<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <withJansi>true</withJansi>
    <encoder>
      <pattern>%msg%n</pattern>
    </encoder>
  </appender>

  <logger name="org.factcast.schema.registry.cli" level="INFO" />

  <root level="ERROR">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>
